{
    "Changelog initialize": {
        "prefix": "cl-Init",
        "body": [
            "# **Change Log** üìúüìù\n",
            "All notable changes to the \"**${1:NameOfTheThing}**\" ${2:WhatItIs/program/extension/API/whatever} will be documented in this file.\n",
            "The format is based on [Keep a Changelog](https://keepachangelog.com/en/1.0.0/) and this project adheres to [Semantic Versioning](https://semver.org/spec/v2.0.0.html).\n",
            "---\n\n$3\n"
        ],
        "description": "The initial structure of the changelog in which you will start building your changelog"
    },
    "Changelog Set new Version": {
        "prefix": "cl-NewVersion",
        "body": [
            "## [**${1:Version}**] - $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE\n",
            "$2"
        ],
        "description": "Set a new version with the version name and the current date."
    },
    "Changelog Yanked": {
        "prefix": "cl-Yanked",
        "body": [
            "[YANKED]"
        ],
        "description": "For versions that had to be pulled because of a serious bug or security issue."
    },
    "Changelog Unreleased": {
        "prefix": "cl-Unreleased",
        "body": [
            "## [Unreleased]"
        ],
        "description": "For the actual changes."
    },
    "Changelog Added": {
        "prefix": "cl-Added",
        "body": [
            "### Added\n",
            "* $1"
        ],
        "description": "Add new features to your project."
    },
    "Changelog Changed": {
        "prefix": "cl-Changed",
        "body": [
            "### Changed\n",
            "* $1"
        ],
        "description": "For changes in existing functionality."
    },
    "Changelog Deprecated": {
        "prefix": "cl-Deprecated",
        "body": [
            "### Deprecated\n",
            "* $1"
        ],
        "description": "For soon-to-be removed features."
    },
    "Changelog Removed": {
        "prefix": "cl-Removed",
        "body": [
            "### Removed\n",
            "* $1"
        ],
        "description": "For removed features."
    },
    "Changelog Fixed": {
        "prefix": "cl-Fixed",
        "body": [
            "### Fixed\n",
            "* $1"
        ],
        "description": "For bug fixes."
    },
    "Changelog Improved": {
        "prefix": "cl-Improved",
        "body": [
            "### Improved\n",
            "* $1"
        ],
        "description": "For feature improvements."
    },
    "Changelog Security": {
        "prefix": "cl-Security",
        "body": [
            "### Security\n",
            "* $1"
        ],
        "description": "For security fixes and vulnerabilities."
    },
    "Changelog Published": {
        "prefix": "cl-Published",
        "body": [
            "[Published]"
        ],
        "description": "Adds a published tag."
    },
    "Link": {
        "prefix": "md-Link",
        "body": [
            "[${1:Title}](${2:URL/URI})"
        ],
        "description": "Link to a file or a web page."
    },
    "Italic": {
        "prefix": "md-Italic",
        "body": [
            "*${1:Text}*"
        ],
        "description": "Adds an Italic text."
    },
    "Bold": {
        "prefix": "md-Bold",
        "body": [
            "**${1:Text}**"
        ],
        "description": "Adds a bold text."
    },
    "Strike through": {
        "prefix": "md-Strike",
        "body": [
            "~~${1:Text}~~"
        ],
        "description": "Strike through your text."
    },
    "List": {
        "prefix": "md-List",
        "body": [
            "* $1",
            "* $2",
            "* $3"
        ],
        "description": "Make a quick 3 elements list."
    },
    "Inline code": {
        "prefix": "md-InlineCode",
        "body": [
            "`${1:YourLineCodeHere}`"
        ],
        "description": "Insert an inline code."
    },
    "Multi line code": {
        "prefix": "md-MultiLineCode",
        "body": [
            "```${1:language}",
            "${2:The code}",
            "```"
        ],
        "description": "Insert multi-line code."
    },
    "H1 - Header one": {
        "prefix": "md-h1",
        "body": [
            "# **${1:YourHeader}**\n",
            "$2"
        ],
        "description": "Insert a header one H1."
    },
    "H2 - Header two": {
        "prefix": "md-h2",
        "body": [
            "## **${1:YourHeader}**\n",
            "$2"
        ],
        "description": "Insert a header two H2."
    },
    "H3 - Header three": {
        "prefix": "md-h3",
        "body": [
            "### **${1:YourHeader}**\n",
            "$2"
        ],
        "description": "Insert a header three H3."
    },
    "H4 - Header four": {
        "prefix": "md-h4",
        "body": [
            "#### **${1:YourHeader}**\n",
            "$2"
        ],
        "description": "Insert a header four H4."
    },
    "H5 - Header five": {
        "prefix": "md-h5",
        "body": [
            "##### **${1:YourHeader}**\n",
            "$2"
        ],
        "description": "Insert a header five H5."
    },
    "H6 - Header six": {
        "prefix": "md-h6",
        "body": [
            "###### **${1:YourHeader}**\n",
            "$2"
        ],
        "description": "Insert a header six H6."
    },
    "CheckBox Unchecked": {
        "prefix": "md-Box",
        "body": [
            "[ ] $1"
        ],
        "description": "Insert an unchecked checkbox."
    }

}
