{
    "Changelog initialize": {
        "prefix": "ChangelogInit",
        "body": [
            "# **Change Log** üìúüìù\n",
            "All notable changes to the \"**${1:NameOfTheThing}**\" ${2:WhatItIs/program/extension/API/whatever} will be documented in this file.\n",
            "The format is based on [Keep a Changelog](https://keepachangelog.com/en/1.0.0/) and this project adheres to [Semantic Versioning](https://semver.org/spec/v2.0.0.html).\n",
            "---\n\n$3\n"
        ],
        "description": "The initial structure of the changelog in which you will start building your changelog"
    },
    "Changelog Set new Version": {
        "prefix": "ChangelogNewVersion",
        "body": [
            "## [${1:Version}] - $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE\n",
            "$2"
        ],
        "description": "Set a new version with the version name and the current date."
    },
    "Changelog Yanked": {
        "prefix": "ChangelogYanked",
        "body": [
            "[YANKED]"
        ],
        "description": "For versions that had to be pulled because of a serious bug or security issue."
    },
    "Changelog Unreleased": {
        "prefix": "ChangelogUnreleased",
        "body": [
            "## [Released]"
        ],
        "description": "For the actual changes."
    },
    "Changelog Added": {
        "prefix": "ChangelogAdded",
        "body": [
            "### Added\n",
            "* $1"
        ],
        "description": "Add new features to your project."
    },
    "Changelog Changed": {
        "prefix": "ChangelogChanged",
        "body": [
            "### Changed\n",
            "* $1"
        ],
        "description": "For changes in existing functionality."
    },
    "Changelog Deprecated": {
        "prefix": "ChangelogDeprecated",
        "body": [
            "### Deprecated\n",
            "* $1"
        ],
        "description": "For soon-to-be removed features."
    },
    "Changelog Removed": {
        "prefix": "ChangelogRemoved",
        "body": [
            "### Removed\n",
            "* $1"
        ],
        "description": "For removed features."
    },
    "Changelog Fixed": {
        "prefix": "ChangelogFixed",
        "body": [
            "### Fixed\n",
            "* $1"
        ],
        "description": "For bug fixes."
    },
    "Changelog Security": {
        "prefix": "ChangelogSecurity",
        "body": [
            "### Security\n",
            "* $1"
        ],
        "description": "For security fixes and vulnerabilities."
    },
    "Link": {
        "prefix": "Link",
        "body": [
            "[${1:Title}](${2:URL/URI})"
        ],
        "description": "Link to a file or to a web page."
    },
    "Italic": {
        "prefix": "Italic",
        "body": [
            "*${1:Text}*"
        ],
        "description": "Adds an Italic text."
    },
    "Bold": {
        "prefix": "Bold",
        "body": [
            "**${1:Text}**"
        ],
        "description": "Adds a bold text."
    },
    "Strike through": {
        "prefix": "Strike",
        "body": [
            "~~${1:Text}~~"
        ],
        "description": "Strike through your text."
    },
    "List": {
        "prefix": "List",
        "body": [
            "* $1",
            "* $2",
            "* $3"
        ],
        "description": "Make a quick 3 elements list."
    },
    "Inline code": {
        "prefix": "InlineCode",
        "body": [
            "`${1:YourLineCodeHere}`"
        ],
        "description": "Insert an inline code."
    },
    "Multi line code": {
        "prefix": "MultiLineCode",
        "body": [
            "```${1:language}",
            "${2:The code}",
            "```"
        ],
        "description": "Insert multi line code."
    }

}